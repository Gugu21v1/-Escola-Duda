<%= simple_form_for [sala, aluno, nota] do |f| %>
  <%= f.error_notification %>
  <%= f.input :trimestre, :collection => { '1° Trimestre' => '1° Trimestre', '2° Trimestre' => '2° Trimestre', '3° Trimestre' => '3° Trimestre', '4° Trimestre' => '4° Trimestre'} %>
  <%= f.association :horario, collection: @nomes %>
  <%= f.input :nota %>
  <%= f.submit %>
<% end %>
